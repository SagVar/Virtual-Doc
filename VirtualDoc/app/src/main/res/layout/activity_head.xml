                android:layout_below="@+id/checkBox72"
                android:layout_alignLeft="@+id/checkBox72"
                android:textSize="20sp"
                android:layout_marginTop="5dp"
                android:onClick="onHeadCheckboxClicked"/>

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="loss of consciousness or awareness"
                android:id="@+id/checkBox74"
                android:layout_below="@+id/checkBox73"
