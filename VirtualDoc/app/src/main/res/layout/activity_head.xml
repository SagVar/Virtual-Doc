                android:text="bleeding"
                android:id="@+id/checkBox76"
                android:layout_below="@+id/checkBox75"
                android:layout_alignLeft="@+id/checkBox75"
                android:textSize="20sp"
                android:layout_marginTop="5dp"
                android:onClick="onHeadCheckboxClicked"/>

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="difficulty in concentration"
                android:id="@+id/checkBox77"
                android:layout_below="@+id/checkBox76"
                android:layout_alignLeft="@+id/checkBox76"
                android:textSize="20sp"
                android:layout_marginTop="5dp"
                android:onClick="onHeadCheckboxClicked"/>

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="pale body"
                android:id="@+id/checkBox78"
                android:layout_below="@+id/checkBox77"
                android:layout_alignLeft="@+id/checkBox77"
                android:textSize="20sp"
                android:layout_marginTop="5dp"
                android:onClick="onHeadCheckboxClicked"/>

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="insomnia"
                android:id="@+id/checkBox62"
                android:layout_below="@+id/checkBox78"
                android:layout_alignLeft="@+id/checkBox78"
